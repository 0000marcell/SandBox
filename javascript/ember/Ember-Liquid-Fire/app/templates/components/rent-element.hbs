<div id="liquid-box-demo">
	<button {{action 'changeVehicle'}}>Change</button>
  <label>Im renting</label>
	{{#liquid-if isBike class="vehicles"}}
    <label>
      Would you like a helmet? {{input type="checkbox" checked="helmet"}}
    </label>
  {{else}}
    <label>License Number</label>
    {{input type="text" value=license}}
    <label>License State</label>
  {{/liquid-if}}

  <button type="button" class="btn btn-success">Yay, lets go!</button>
</div>

