<thead>
  <tr>
    {{#each columnComponents as |column|}}
      {{component column.headerComponentName column=column}}
    {{/each}}
  </tr>
</thead>
<tbody>
	{{#each content as |row|}}
		<tr>
			{{#each elements as |child|}}
				{{component child.type row=row 
					valueBindingPath=child.key opts=child.opts}}
			{{/each}}
		</tr>	
	{{/each}}
</tbody>
