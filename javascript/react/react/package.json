  "bugs": {
    "url": "https://github.com/0000marcell/react-sandbox/issues"
  },
  "homepage": "https://github.com/0000marcell/react-sandbox",
  "devDependencies": {
    "babelify": "^7.0.2",
    "express": "^4.13.3"
  },
  "dependencies": {
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "skeleton-css": "^2.0.4"
  }
}
